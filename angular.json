{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "InstitutionalSite": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "css"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/InstitutionalSite",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/stylesCustom",
                            "src/web.config"
                        ],
                        "styles": [
                            "src/stylesCustom/styles/bootstrap/bootstrap.css",
                            "src/stylesCustom/styles/cpau.css",
                            "src/stylesCustom/styles/carousel.css",
                            "src/stylesCustom/styles/fontawesome/font-awesome.css"
                        ],
                        "scripts": [
                            "src/stylesCustom/js/scrollup/lib/jquery-1.11.1.min.js",
                            "src/stylesCustom/js/boostrap/bootstrap.min.js",
                            "src/stylesCustom/js/carousel.js",
                            "src/stylesCustom/js/recortar.js",
                            "src/stylesCustom/js/imageLinkStyle.js",
                            "src/stylesCustom/js/playerAPI.js",
                            "src/stylesCustom/js/scrollup/jquery.scrollUp.min.js",
                            "src/stylesCustom/js/scrollup/scrollUp.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "30kb",
                                    "maximumError": "400kb"
                                }
                            ]
                        },
                        "test": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.test.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "30kb",
                                    "maximumError": "400kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "InstitutionalSite:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "InstitutionalSite:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "InstitutionalSite:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "InstitutionalSite:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "InstitutionalSite:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "InstitutionalSite"
}